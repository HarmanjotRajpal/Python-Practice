s = 'rat'
t = 'cary'
d1 = {}
d2 = {}
for i in range(0, len(s)):
    if s[i] not in d1:
        d1[s[i]]= 1 
    else:
        d1[s[i]] = d1[s[i]] + 1 

print(d1)

for i in range(0, len(t)):
    if t[i] not in d2:
        d2[t[i]] = 1 
    else:
        d2[t[i]] = d2[t[i]] + 1
print(d2)

if (len(d1)!=len(d2)):
    print('length if not anagram')
else:
    if(d1==d2):
        print('anagram')
    else:
        print('not anagram')
