def isPrime(num):
    #print("n is: ", num)
    if n <= 1:
        return False
    
    for i in range(2, num):
        #print(" i = ", i)
        if(num%i ==0):
            return False
    return True
    

def printPrime(n):
    for num in range(2, n+1):
        #print(" in print: ", i)
        if isPrime(num):
            print(i,  " ")

n =7
print(printPrime(n))
